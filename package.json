{
    "name": "ton-solidity",
    "displayName": "Everscale(TON) Solidity Support",
    "description": "Everscale Solidity Language for Visual Studio Code",
    "version": "1.1.36",
    "keywords": [
        "solidity",
        "ton-solidity",
        "Everscale",              
        "blockchain",
        "tondev",
        "highlighting"
    ],
    "publisher": "zfreeton",
    "license": "Apache-2.0",
    "licenses": [
        {
            "type": "Apache-2.0",
            "url": "http://www.apache.org/licenses/LICENSE-2.0"
        }
    ],
    "engines": {
        "vscode": "^1.59.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/freeton20/ton-highlight"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "activationEvents": [
        "onLanguage:ton-solidity"
    ],
    "main": "./src/extension.js",
    "contributes": {
        "languages": [
            {
                "id": "ton-solidity",
                "aliases": [
                    "ton-solidity"
                ],
                "extensions": [
                    ".sol"
                ],
                "configuration": "./ton-solidity.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "ton-solidity",
                "scopeName": "source.ton-solidity",
                "path": "./src/syntaxes/ton-solidity.json"
            }
        ]
    },
    "icon": "assets/icon.png",
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "node ./test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.2.2",
        "@types/node": "14.x",
        "@types/vscode": "^1.59.0",
        "eslint": "^7.32.0",
        "glob": "^7.1.7",
        "mocha": "^8.4.0",
        "typescript": "^4.3.2",
        "vscode-test": "^1.5.2"
    },
    "dependencies": {
        "strip-comments": "^2.0.1",
        "tondev": "^0.11.1"
    }
}
